{
	"extends": ["react-app", "react-app/jest"],
	"rules": {
		"no-console": "warn",
		"import/order": [
			"warn",
			{
				"groups": [
					"builtin",
					"external",
					"internal",
					"parent",
					"sibling",
					"index",
					"object",
					"type"
				],
				"newlines-between": "always-and-inside-groups"
			}
		]
	}
}
